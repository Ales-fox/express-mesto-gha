{
  "env": {
      "browser": false,
      "es6": true,
      "mongo":true,
      "node": true
  },
  "extends": [
      "eslint:recommended",
      "airbnb-base"
  ],
  "parser": "@babel/eslint-parser",
  "parserOptions": {
      "ecmaVersion": 11,
      "sourceType": "module",
      "requireConfigFile": false
  },
  "rules": {
  "func-names": "off",
	"no-underscore-dangle": [
		"error",
		{
			"allow": ["_id"]
		}
	],
	"no-console": "off",
  "semi": [2, "always"],
  "allowAllPropertiesOnSameLine": "off"
	}
}
