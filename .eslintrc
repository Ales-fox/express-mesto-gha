{
  "env": {
      "browser": true,
      "es2021": true
  },
  "extends": [
      "eslint:recommended",
      "airbnb-base"
  ],
  "parser": "@babel/eslint-parser",
  "parserOptions": {
      "ecmaFeatures": {
          "jsx": true
      },
      "ecmaVersion": 11,
      "sourceType": "module",
      "requireConfigFile": false
  },

  "rules": {
	"no-underscore-dangle": [
		"error",
		{
			"allow": ["_id"]
		}
	],
	"no-console": "off",
  "semi": [2, "always"],
  "allowAllPropertiesOnSameLine": "off"
	}
}
